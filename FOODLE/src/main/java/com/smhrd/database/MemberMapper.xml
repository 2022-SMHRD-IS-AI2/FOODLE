<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.smhrd.model.MemberDAO">
	
	<insert id="join" parameterType="com.smhrd.model.MemberVO">
		insert into TBL_MEMBER values (#{mb_id}, #{mb_pw}, #{mb_name}, #{mb_email}, #{mb_phone}, #{mb_bloodtype}, #{mb_height}, #{mb_weight}, 'N', sysdate)
	</insert>
	
	
	

</mapper>